<body>
    <h2>Change Color</h2>
    <button onclick="changeColor()">Click Me </button>
    <script>
        function changeColor(){

        
        document.body.style.backgroundColor = "#"+ Math.floor(Math.random()*1436568).toString(16);
        }
    </script>
</body>
</html>
